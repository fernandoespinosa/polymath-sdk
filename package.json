{
  "name": "@polymathnetwork/sdk",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "build"
    ],
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/src/testUtils/setupTests.ts"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "bignumber.js": "^8.0.1",
    "semver": "^5.6.0",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.30"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/semver": "^5.5.0",
    "@types/uuid": "^3.4.4",
    "@types/web3": "^1.0.14",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "reflect-metadata": "^0.1.12",
    "regenerator-runtime": "^0.13.1",
    "typescript": "3.2.4",
    "web3-core-promievent": "^1.0.0-beta.37",
    "semantic-release": "^15.13.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandoespinosa/polymath-sdk.git"
  },
  "release": {
    "branch": "release-branch"
  }
}